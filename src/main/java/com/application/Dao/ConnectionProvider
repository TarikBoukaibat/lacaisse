package DAO;

import java.sql.*;

public class ConnectionProvider {
    public  static Connection getCon(){
      try {
           Class.forName("com.mysql.jdbc.Driver");
          Class.forName("com.msql.jdbc..Driver");
          Connection con = DriverManager.getConnection("jdbc:msql://localhost:3306/CoffeDB");
          return con;
      }catch (Exception e){
          return null;
      }

    }




}
